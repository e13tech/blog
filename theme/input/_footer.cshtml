<ul class="icons">
    @foreach (IDocument rssFeed in Outputs["**/*.rss"]) {
    <li><a href="@rssFeed.GetLink()" class="fas fa-rss"><span class="label">@rssFeed.GetString(WebKeys.Title)</span></a></li>
    }
    @foreach (IDocument atomFeed in Outputs["**/*.atom"]) {
    <li><a href="@atomFeed.GetLink()" class="fas fa-atom"><span class="label">@atomFeed.GetString(WebKeys.Title)</span></a></li>
    }
    <li><a href="https://www.reddit.com/user/jjbussert" class="icon brands fa-reddit"><span class="label">Reddit</span></a></li>
    <li><a href="https://github.com/JJBussert" class="icon brands fa-github"><span class="label">Github</span></a></li>
    <li><a href="https://twitter.com/JJ_Bussert" class="icon brands fa-twitter"><span class="label">Twitter</span></a></li>
    <li><a href="https://www.linkedin.com/in/jjbussert/" class="icon brands fa-linkedin-in"><span class="label">LinkedIn</span></a></li>
</ul>
